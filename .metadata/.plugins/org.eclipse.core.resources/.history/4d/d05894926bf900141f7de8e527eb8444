
abstract class Animal {
	String name;
	String ground;
	public int age;
	public int health;
	
	abstract public void live(String location);
	abstract public void speicalskill(Animal a);
	
	Animal(String name){
		this.name = name;
	}
	
	Animal(){}
	
	int getAge(){return this.age;};
	int getWeight(){return this.health;};
	
	public void setAge(int age) {
		if (age >0 && age < 100)
		this.age = age;
		else System.out.println("Wrong value");
	}
	
	public void setWeight(int health) {
		if (health >0 && health < 100)
		this.health = health;
		else System.out.println("Wrong value");
	}

}
