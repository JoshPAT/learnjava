
public class RaceDemo {
	public static String rand;
	static void m1(Runnable obj){obj.run(rand);}
	
	public static void main(String[] args) {
		Person p = new Person("Lucy", 10);
		Dog d = new Dog("Zipper", 12);
		for (int a = 0; a <3; a++){
			String rand = "swamp";
			m1(p);
			m1(d);
		}
		
	}
}
