
public class RaceDemo {
	
	
	
	
	
	public static void main(String[] args) {
		RaceDemo I = new RaceDemo();
		Person p = new Person("Lucy", 10);
		Dog d = new Dog("Zipper", 12);
		
		for (int a = 0; a <3; a++){
			I.r1(p);
			I.r1(d);
			I.r2(p);
			I.r2(p);
		}
		p.speicalskill(d);
	
	}
	
	void r1(Runnable obj){obj.run("grass");}
	void r2(Runnable obj){obj.run("swamp");}
	
}
