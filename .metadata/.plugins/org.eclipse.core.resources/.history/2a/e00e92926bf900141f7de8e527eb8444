
public class Wolf extends Animal implements Runnable, Swimmable, Jumpable {
	private int age;
	private int weight;
	public int hungry;
	
	@Override
	public void speicalskill(Animal a) {
		System.out.print(name +" attacks " + a.name);
		
	}		

	public void jump() {
		
		
	}
	
	public void run(String ground) { 
		this.ground = ground;
		System.out.println(name +" is running on the "+ ground); }
	
	public void swim(String ground)  { 
		this.ground = ground;
		System.out.print(name +" is swimming in the "+ ground); }
	
	public int getSwimspeed(){
		if (ground == "water")
			return (int) (50-(0.7*super.health)-(0.3*super.age));
		else if (ground == "swamp")
			return (int) (30-(0.7*super.health)-(0.3*super.age));
		return (int) 0;
	}
		
	public int getRunspeed(){
		if (ground == "grass")
			return (int) (90-(0.7*super.health)-(0.3*super.age));
		else if (ground == "sand")
			return (int) (70-(0.7*super.health)-(0.3*super.age));
		return (int) 0;};
		
	Wolf(String name,int hungry){
			super(name);
			this.hungry = hungry;
	}	
	
	
	public static void main(String[] args) {
		Wolf d = new Wolf("Zipper", (int) (Math.random()*10+1));
		d.setWeight(7);
		d.run("swamp");
		System.out.println(d.getSwimspeed());
		System.out.println(d.getWeight());
		
	}

	@Override
	public void live(String location) {
		// TODO Auto-generated method stub
		
	}	
	
}
